{
	"environ": {
		"TMP": "/tmp",
		"RT_SYS": "${TMP}/gnunet-system-runtime"
	},
	"dht": {
		"endpoint": "unix+${RT_SYS}/gnunet-service-dht.sock"
	},
	"gns": {
		"endpoint": "unix+${RT_SYS}/gnunet-service-gns-go.sock+perm=0770",
		"dhtReplLevel": 10
	},
	"namecache": {
		"endpoint": "unix+${RT_SYS}/gnunet-service-namecache.sock"
	}
}
